{
  "name": "mdx-list-tables",
  "version": "1.0.0",
  "description": "MDX component for creating tables with merged cells using list syntax",
  "main": "dist/ListTable.js",
  "types": "dist/ListTable.d.ts",
  "exports": {
    ".": {
      "types": "./dist/ListTable.d.ts",
      "import": "./dist/ListTable.js",
      "require": "./dist/ListTable.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "docs:dev": "cd docs && npm start",
    "docs:build": "cd docs && npm run build",
    "docs:serve": "cd docs && npm run serve"
  },
  "keywords": [
    "mdx",
    "markdown",
    "table",
    "list-table",
    "react"
  ],
  "author": "Ben Rose <daeruin@benjaminrose.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@mdx-js/mdx": "^3.1.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/jest-axe": "^3.5.9",
    "@types/mdx": "^2.0.10",
    "@types/react": "^18.3.27",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
